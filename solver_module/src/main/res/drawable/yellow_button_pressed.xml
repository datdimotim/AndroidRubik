<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <corners android:radius="15dp" />
            <gradient
                android:angle="90"
                android:endColor="#444400"
                android:gradientRadius="40dp"
                android:startColor="#999900"
                android:type="radial">
            </gradient>
            <stroke
                android:width="3dp"
                android:color="#000000">
            </stroke>
            <size
                android:width="60dp"
                android:height="60dp">
            </size>
        </shape>
    </item>
</layer-list>